{
  "name": "boilerplate-react-vite",
  "version": "0.0.0",
  "description": "Boilerplate for React + Vite",
  "private": true,
  "scripts": {
    "dev": "vite --port 3000",
    "dev:dev": "env-cmd -f .env.development --fallback --no-override vite --port 3000",
    "build": "npm run build:prod",
    "build:dev": "tsc && env-cmd -f .env.development --fallback --no-override vite build",
    "build:prod": "tsc && vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "react": "~18.0.0",
    "react-dom": "~18.0.0",
    "react-helmet-async": "~1.3.0",
    "web-vitals": "~2.1.4"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "~0.3.1",
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~13.0.1",
    "@testing-library/user-event": "~14.1.0",
    "@types/node": "~17.0.24",
    "@types/react": "~18.0.5",
    "@types/react-dom": "~18.0.1",
    "@vitejs/plugin-react": "~1.3.1",
    "autoprefixer": "~10.4.4",
    "cross-env": "~7.0.3",
    "env-cmd": "~10.1.0",
    "postcss": "~8.4.12",
    "postcss-import": "~14.1.0",
    "postcss-nested": "~5.0.6",
    "tailwindcss": "~3.0.24",
    "typescript": "~4.6.3",
    "vite": "~2.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "test": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
